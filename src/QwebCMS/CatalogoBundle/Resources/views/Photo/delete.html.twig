{% for flashMessage in app.session.flashbag.get('notice') %}
    <script>
        $(document).ready(function(){
            swal({
                title: "Cancellato!",
                text: "{{ flashMessage }}",
                type: "success"
            });
        });
    </script>
{% endfor %}

{% for flashMessage in app.session.flashbag.get('error') %}
    <script>
        $(document).ready(function(){
            swal({
                title: "Errore!",
                text: "{{ flashMessage }}",
                type: "error"
            });
        });
    </script>
{% endfor %}